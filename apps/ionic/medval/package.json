{
  "name": "medval",
  "author": "Chinmay Nagarkar",
  "homepage": "http://healthcaretech.io/",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "cleancss": "ionic-app-scripts cleancss // Doesn't do much.",
    "android:prod": "npm run web:prod && cordova build android --release",
    "ios:prod": "npm run web:prod && cordova build ios --release",
    "web:prod": "npm run ionic:build && npm run uglify:deljsmap",
    "uglify": "uglifyjs www/build/main.js -c keep_fnames=true,unused,dead_code -o www/build/main.js",
    "deljsmap": "del www/build/main.js.map",
    "uglify:deljsmap": "npm run uglify && npm run deljsmap",
    "imagemin": "imagemin www/assets/img/* --out-dir=www/assets/imgmin // Doesn't do much",
    "minify": "ionic-app-scripts minify // Doesn't work: Runs uglify internally but no way to pass custom options like keep_fnames"
  },
  "dependencies": {
    "@angular/common": "2.2.1",
    "@angular/compiler": "2.2.1",
    "@angular/compiler-cli": "2.2.1",
    "@angular/core": "2.2.1",
    "@angular/forms": "2.2.1",
    "@angular/http": "2.2.1",
    "@angular/platform-browser": "2.2.1",
    "@angular/platform-browser-dynamic": "2.2.1",
    "@angular/platform-server": "2.2.1",
    "@ionic/storage": "1.1.7",
    "ionic-angular": "2.0.0-rc.4",
    "ionic-native": "2.2.11",
    "chart.js": "^2.3.0",
    "ionicons": "3.0.0",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.26"
  },
  "devDependencies": {
    "@ionic/app-scripts": "0.0.47",
    "del": "^2.2.2",
    "imagemin-cli": "^3.0.0",
    "source-map-explorer": "^1.3.3",
    "typescript": "2.0.9",
    "uglify-js": "^2.7.5"
  },
  "config": {
    "ionic_uglifyjs":"-c keep_fnames=true,unused,dead_code"
  },
  "description": "medval: An Ionic project",
  "cordovaPlugins": [
    "cordova-plugin-device",
    "cordova-plugin-console",
    "cordova-plugin-whitelist",
    "cordova-plugin-splashscreen",
    "cordova-plugin-statusbar",
    "cordova-plugin-inappbrowser",
    "ionic-plugin-keyboard"
  ],
  "cordovaPlatforms": [
    "ios",
    {
      "platform": "ios",
      "version": "",
      "locator": "ios"
    }
  ]
}
