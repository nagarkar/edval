{
  "name": "medval",
  "author": "Chinmay Nagarkar",
  "homepage": "http://healthcaretech.io/",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "e2e": "protractor",
    "postinstall": "ionic state restore && webdriver-manager update",
    "start": "ionic serve",
    "test": "ng test",
    "test-coverage": "ng test --code-coverage",
    "cleancss": "ionic-app-scripts cleancss // Doesn't do much.",
    "android:prod": "npm run web:prod && cordova build android --release",
    "ios:prod": ". ./scripts/gitpull.sh && npm run build && npm run uglify:deljsmap && cordova build ios --release",
    "web:prod": "npm run uglify:deljsmap",
    "uglify:deljsmap": "npm run uglify && npm run deljsmap",
    "uglify": "npm run uglifymain && npm run uglifyawssdk",
    "deljsmap": "del www/build/*.js.map www/assets/js/*.js.map",
    "uglifymain": "uglifyjs www/build/main.js -c keep_fnames=true,unused,dead_code -o www/build/main.js",
    "uglifyawssdk": "uglifyjs www/assets/js/aws-sdk.js -c keep_fnames=true,unused,dead_code -o www/assets/js/aws-sdk.js",
    "imagemin": "imagemin www/assets/img/* --out-dir=www/assets/imgmin // Doesn't do much",
    "minify": "ionic-app-scripts minify // Doesn't work: Runs uglify internally but no way to pass custom options like keep_fnames"
  },
  "dependencies": {
    "@angular/common": "2.2.1",
    "@angular/compiler": "2.2.1",
    "@angular/compiler-cli": "2.2.1",
    "@angular/core": "2.2.1",
    "@angular/forms": "2.2.1",
    "@angular/http": "2.2.1",
    "@angular/platform-browser": "2.2.1",
    "@angular/platform-browser-dynamic": "2.2.1",
    "@angular/platform-server": "2.2.1",
    "@ionic/storage": "1.1.7",
    "@ng-idle/core": "^2.0.0-beta.4",
    "ionic-angular": "2.0.0-rc.4",
    "ionic-native": "2.2.11",
    "ionicons": "3.0.0",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.26"
  },
  "devDependencies": {
    "@ionic/app-scripts": "0.0.47",
    "@types/jasmine": "^2.5.40",
    "@types/node": "^6.0.51",
    "angular-cli": "^1.0.0-beta.24",
    "codecov": "^1.0.1",
    "del": "^2.2.2",
    "del-cli": "^0.2.1",
    "imagemin-cli": "^3.0.0",
    "jasmine-core": "^2.5.2",
    "jasmine-spec-reporter": "^3.0.0",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.1",
    "karma-remap-istanbul": "^0.4.0",
    "protractor": "^4.0.14",
    "protractor-jasmine2-screenshot-reporter": "^0.3.2",
    "source-map-explorer": "^1.3.3",
    "ts-node": "^2.0.0",
    "tslint": "^4.3.1",
    "tslint-eslint-rules": "^3.2.3",
    "typescript": "latest",
    "uglify-js": "^2.7.5"
  },
  "config": {
    "ionic_uglifyjs": "-c keep_fnames=true,unused,dead_code"
  },
  "description": "medval: An Ionic project",
  "cordovaPlugins": [
    "cordova-plugin-device",
    "cordova-plugin-console",
    "cordova-plugin-whitelist",
    "cordova-plugin-splashscreen",
    "cordova-plugin-statusbar",
    "cordova-plugin-inappbrowser",
    "ionic-plugin-keyboard"
  ],
  "cordovaPlatforms": [
    "ios",
    {
      "platform": "ios",
      "version": "",
      "locator": "ios"
    }
  ]
}
